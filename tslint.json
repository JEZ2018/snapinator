{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-classes-per-file": false,
    "member-access": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "functions": "never"
        }
      }
    ]
  },
  "rulesDirectory": []
}
